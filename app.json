{
  "expo": {
    "name": "Cusco Go",
    "slug": "cusco-go",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cuscogo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#F2A900"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.cuscogo.app",
      "buildNumber": "2",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow camera access to capture scavenger hunt photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow saving captured photos and thumbnails to your Photos library.",
        "NSPhotoLibraryUsageDescription": "Allow selecting images from your Photos library.",
        "NSLocationWhenInUseUsageDescription": "Allow recording GPS coordinates with your captures while using the app.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F2A900"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#F2A900",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e51973b3-e611-4376-a144-dd08c71c9b14"
      }
    }
  }
}
